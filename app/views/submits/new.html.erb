<% @page_title = "New submit" %>


<div class="submits new">
<br />
  <h2>Submit Your Photo</h2>

  <%= form_for(:submit, :url => {:action => 'create'}, :html=>{:multipart=>true}) do |f| %>

    <%= render(:partial => "form", :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= submit_tag("Submit") %>
    </div>

  <% end %>
</div>