<% @page_title = "New Jean" %>


<div class="Jeans new">
  <h2>Create New Jean Model</h2>

  <%= form_for(:jean, :url => {:action => 'create'}, :html=>{:multipart=>true}) do |f| %>

    <%= render(:partial => "form", :locals => {:f => f}) %>
    
	<br />
    <div class="form-buttons">
      <%= submit_tag("Create Jean Model") %>
    </div>

  <% end %>
</div>